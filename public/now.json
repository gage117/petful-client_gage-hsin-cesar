{
  "version": 2,
  "name": "Petful",
  "alias": "Petful",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/favicon.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json$",
      "dest": "/manifest.json"
    },
    {
      "src": "^/site.webmanifest.json$",
      "dest": "/site.webmanifest.json"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
    "REACT_APP_API_ENDPOINT": "https://obscure-sierra-79316.herokuapp.com/api"
    }
  }
}
 